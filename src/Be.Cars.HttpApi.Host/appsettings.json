{
  "App": {
    "SelfUrl": "https://localhost:44344",
    "CorsOrigins": "https://*.Cars.com",
    "DisablePII": false,
    "HealthCheckUrl": "/health-status"
  },
  "ConnectionStrings": {
    "Default": "Server=monster;Database=Cars;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
  "AuthServer": {
    "Authority": "https://localhost:44339",
    "RequireHttpsMetadata": true,
    "SwaggerClientId": "Cars_Swagger"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "CGSU1IGZR301k3ty"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "OpenTelemetry": {
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    }
  },
  "ServiceName": "TJILP-HttpApiHost",
  "AllowedHosts": "*",
  "UseTracingExporter": "console",
  "UseMetricsExporter": "console",
  "UseLogExporter": "console",
  "HistogramAggregation": "explicit",
  "Otlp": {
    "Endpoint": "http://localhost:4317"
  }
}
